{
  "name": "inspecto",
  "version": "1.0.0",
  "description": "Molecules checker",
  "main": "build/inspecto.umd.cjs",
  "source": "index.ts",
  "type": "module",
  "module": "build/inspecto.js",
  "files": ["build"],
  "exports": {
    ".": {
      "import": "./build/inspecto.js",
      "require": "./build/inspecto.cjs"
    },
    "./rules": {
      "import": "./build/rules.js",
      "require": "./build/rules.cjs"
    },
    "./utils": {
      "import": "./build/utils.js",
      "require": "./build/utils.cjs"
    }
  },
  "scripts": {
    "build": "npm run build:bundle",
    "build:bundle": "vite build",
    "lint": "eslint index.ts src/**/*.ts",
    "prepare": "husky && husky install",
    "demo": "vite --port 8000"
  },
  "repository": {
    "type": "git",
    "url": "https://git.epam.com/epm-lstr/epm-lstr-chc/inspecto/-/tree/Trunk"
  },
  "keywords": ["epam", "inspecto", "molecules"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.10",
    "inversify": "^6.0.2",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.1",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "indigo-ketcher": "^1.16.0",
    "reflect-metadata": "^0.2.1",
    "vite": "^5.0.12"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.{ts,md}": "prettier --write"
  }
}
